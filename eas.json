{
  "cli": {
    "version": ">=16.0.0"
  },
  "build": {
    "production": {
      "node": "22.20.0",
      "android": {
        "buildType": "app-bundle",
        "developmentClient": false,
        "credentialsSource": "remote",
        "autoSubmit": true,
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "preview": {
      "android": {
        "buildType": "app-bundle",
        "developmentClient": false,
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "production",
        "releaseStatus": "draft"
      }
    }
  }
}
