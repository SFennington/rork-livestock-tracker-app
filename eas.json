{
  "cli": {
    "version": ">=16.0.0"
  },
  "build": {
    "production": {
      "node": "20.18.0",
      "env": {
        "EAS_BUILD_PACKAGE_MANAGER": "npm"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "node": "20.18.0",
      "env": {
        "EAS_BUILD_PACKAGE_MANAGER": "npm"
      },
      "android": {
        "buildType": "app-bundle",
        "developmentClient": false,
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    }
  }
}
