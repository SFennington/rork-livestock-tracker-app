{
  "cli": {
    "version": ">=3.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "developmentClient": false,
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "google-play-key.json",
        "track": "internal"
      }
    }
  }
}
